{
    "name": "php-basic-repo"
    "version": "0.1"
    "description": "data repository with unit of work",
    "type": "library",
    "config": {
		"secure-http": false
    },
	"source": {
		"type": "git",
		"url": "git://data.shanknet.local:/usr/local/git/composer/base/php-basic-repo/",
		"reference": "master"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "http://web.shanknet.local/composer/repo"
		}
	],
	"require-dev": {
		"phpunit/phpunit": "^5.4"
	},
	"require": {
		"php-basic-patterns": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"base\\data\\exception\\": "src/exception/",
			"base\\data\\repository\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"base\\data\\test\\": "tests/"
		}
	},
	"keywords": [
	],
	"license": "GPL-3.0+",
	"authors": [
		{
			"name": "Claus Petersen",
			"role": "Developer"
		}
	]
}
